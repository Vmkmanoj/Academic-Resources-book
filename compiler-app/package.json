{
  "name": "@kongu/compiler-app",
  "version": "0.0.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "dist/esm/index.js",
      "require": "dist/cjs/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --module commonjs --target es5 --jsx react --declaration --outDir dist/cjs && tsc --module esnext --target es5 --jsx react --declaration --outDir dist/esm",
    "watch": "tsc --watch --preserveWatchOutput",
    "dev-watch": "concurrently \"npm run dev\" \"npm run watch\""
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "antd": "^5.22.5",
    "axios": "^1.7.9",
    "rc-picker": "^4.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.10"
  }
}